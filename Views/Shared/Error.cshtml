@model HandleErrorInfo

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <hgroup>
        <h1>Error.</h1>
        <h2>An error occurred while processing your request.</h2>

        <h5>@Model.Exception.Message</h5>

        <p>@Model.Exception.StackTrace</p>
    </hgroup>
</body>
</html>
